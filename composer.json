    "type": "library",
    "keywords": ["slug", "url", "friendly"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bobby Allen",
            "email": "ballen@bobbyallen.me",
            "homepage": "http://bobbyallen.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*"
    },
    "autoload": {
        "psr-4": {
            "Ballen\\Sluginator\\": "lib/"
        }
    }
}